{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "title",
        "salary",
        "company_handle"
    ],
    "properties": {
        "title": {
            "type": "string",
            "default": "",
            "title": "The title Schema",
            "examples": [
                "Accommodation manager"
            ]
        },
        "salary": {
            "type": "integer",
            "default": 0,
            "title": "The salary Schema",
            "examples": [
                126000
            ]
        },
        "equity": {
            "type": "null",
            "default": null,
            "title": "The equity Schema",
            "examples": [
                null
            ]
        },
        "companyHandle": {
            "type": "string",
            "default": "",
            "title": "The companyHandle Schema",
            "examples": [
                "mejia-scott-ryan"
            ]
        }
    },
    "examples": [{
        "title": "Accommodation manager",
        "salary": 126000,
        "equity": null,
        "companyHandle": "mejia-scott-ryan"
    }]
}